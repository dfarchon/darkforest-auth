{
  "name": "darkforest-auth",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "description": "Darkforest Auth Server",
  "keywords": [
    "Darkforest",
    "Auth",
    "Server"
  ],
  "scripts": {
    "format": "prettier --write '{packages,server}/**/*'",
    "lint": "pnpm --recursive run lint",
    "lint:eslint": "pnpm --recursive run lint:eslint",
    "lint:stylelint": "pnpm --recursive run lint:stylelint",
    "lint:typecheck": "pnpm --recursive run lint:typecheck",
    "test": "pnpm --recursive run tes"
  },
  "devDependencies": {
    "@darkforest-auth/eslint-config": "workspace:*",
    "@darkforest-auth/ts-config": "workspace:*",
    "@eslint/js": "9.24.0",
    "@types/mocha": "10.0.10",
    "@types/node": "22.14.0",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-regexp": "2.7.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unicorn": "58.0.0",
    "globals": "16.0.0",
    "mocha": "11.1.0",
    "prettier": "3.5.3",
    "type-fest": "4.39.1",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "engines": {
    "node": "22.14.0",
    "pnpm": "10.7.1"
  },
  "packageManager": "pnpm@10.7.1"
}
